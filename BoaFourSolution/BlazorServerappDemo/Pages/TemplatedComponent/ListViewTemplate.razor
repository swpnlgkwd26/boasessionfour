
@typeparam TItem

<ul>
   @foreach (var item in Items)
   {
      @ItemTemplate(item);
   }
</ul>

@code {
    [Parameter] // Expecting  HTML Content From another component
    public RenderFragment<TItem> ItemTemplate { get; set; }

    [Parameter]
    public IReadOnlyList<TItem> Items { get; set; }


}
